{
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontFamily": "Fira Code",
  "terminal.integrated.fontFamily": "Iosevka",
  "editor.fontLigatures": true,
  "window.menuBarVisibility": "compact",
  "window.titleBarStyle": "custom",
  "C_Cpp.default.intelliSenseMode": "linux-gcc-x64",
  "python.showStartPage": false,
  "editor.rulers": [
    100
  ],
  "vim.useSystemClipboard": true,
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.leader": "<Space>",
  "vim.surround": true,
  "vim.camelCaseMotion.enable": true,
  "vim.highlightedyank.enable": true,
  "vim.handleKeys": {
    "<C-.>": false
  },
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<C-k>"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "j"
      ],
      "after": [
        "g",
        "j"
      ]
    },
    {
      "before": [
        "k"
      ],
      "after": [
        "g",
        "k"
      ]
    },
    {
      "before": [
        "<leader>",
        "s"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "K"
      ],
      "commands": [
        "editor.action.showHover"
      ]
    },
    {
      "before": [
        "<leader>",
        "p"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<leader>",
        ";"
      ],
      "commands": [
        "workbench.action.showAllEditors"
      ]
    },
    {
      "before": [
        "<leader>",
        "a"
      ],
      "commands": [
        "editor.action.quickFix"
      ]
    },
    {
      "before": [
        "L"
      ],
      "after": [
        "$"
      ]
    },
    {
      "before": [
        "H"
      ],
      "after": [
        "^"
      ]
    },
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<C-k>"
      ],
      "after": [
        "<Esc>"
      ]
    },
    {
      "before": [
        "<C-h>"
      ],
      "after": [
        ":nohl"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<C-k>"
      ],
      "after": [
        "<Esc>"
      ]
    },
    {
      "before": [
        "<C-h>"
      ],
      "after": [
        ":nohl"
      ]
    }
  ],
  "vim.operatorPendingModeKeyBindings": [
    {
      "before": [
        "L"
      ],
      "after": [
        "$"
      ]
    },
    {
      "before": [
        "H"
      ],
      "after": [
        "^"
      ]
    }
  ],
  "go.toolsManagement.autoUpdate": true,
  "haskell.formattingProvider": "brittany",
  "clang-format.assumeFilename": ".clang-format",
  "git.confirmSync": false,
  "rust-analyzer.completion.addCallArgumentSnippets": false,
  "rust-analyzer.checkOnSave.command": "clippy",
  "redhat.telemetry.enabled": false,
  "[c]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "editor.formatOnPaste": true,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "auto-close-tag.activationOnLanguage": [
    "markdown",
    "xml",
    "php",
    "blade",
    "ejs",
    "jinja",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "plaintext",
    "markdown",
    "vue",
    "liquid",
    "erb",
    "lang-cfml",
    "cfml",
    "HTML (EEx)",
    "HTML (Eex)",
    "plist"
  ],
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "rust-client.engine": "rust-analyzer",
  "remote.SSH.showLoginTerminal": true,
  "remote.SSH.useLocalServer": false,
  "remote.SSH.remotePlatform": {
    "65.1.182.122": "linux",
    "192.168.29.108": "linux",
    "192.168.29.46": "linux",
    "192.168.29.47": "linux"
  },
  "cSpell.userWords": [
    "codebases",
    "comparision",
    "concat",
    "cpuid",
    "deallocation",
    "dknite",
    "eval",
    "getset",
    "LLVM",
    "Majumder",
    "Malloc",
    "modellable",
    "Modellling",
    "peekable",
    "pseudocode",
    "repr",
    "thiserror",
    "uncurry"
  ],
  "ruby.useLanguageServer": true,
  "terminal.integrated.commandsToSkipShell": [
    "language-julia.interrupt"
  ],
  "julia.symbolCacheDownload": true,
  "julia.enableTelemetry": true,
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "cSpell.language": "en-GB",
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.linux": "/home/dknite/.config/nvim/init.vim",
  "editor.formatOnSave": true,
  "telemetry.telemetryLevel": "off",
  "workbench.colorTheme": "GitHub Dark",
  "mesonbuild.configureOnOpen": false,
  "zigLanguageClient.path": "/home/dknite/.local/bin/zls",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[django-html]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.inlayHints.fontFamily": "Courier New",
  "editor.inlayHints.fontSize": 11,
  "workbench.colorCustomizations": {
    "editorInlayHint.background": "#00000000",
    "editorInlayHint.parameterBackground": "#00000000",
    "editorInlayHint.typeBackground": "#00000000"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "files.associations": {
    "*.json.golden": "json",
    "*.rs.golden": "rust",
    "*.cc.golden": "cpp",
    "*.h.golden": "cpp",
    "*.go.golden": "go",
    "*.dart.golden": "dart",
  },
  "haskell.manageHLS": "GHCup",
}